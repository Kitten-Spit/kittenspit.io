{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "esnext",
      "dom"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": false,
    "noEmit": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "webpack"
  ]
}
