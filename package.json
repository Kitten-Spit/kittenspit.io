{
  "name": "kitten-spit",
  "version": "0.0.1",
  "description": "ecommerce web application boilerplate developed for kittenspit.io",
  "scripts": {
    "setup-app": "chmod +x scripts/setup_app.sh && ./scripts/setup_app.sh",
    "serve-app": "chmod +x scripts/serve_app.sh && ./scripts/serve_app.sh",
    "dev-payments": "tsc ./src/payments.ts --outFile ./pub/static/assets/js/payments.js",
    "dev-soft-ui": "tsc ./src/soft-design-system.ts --outFile ./pub/static/assets/js/soft-design-system.js",
    "clean": "chmod +x scripts/cleanup.sh && ./scripts/cleanup.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jesssullivan/kittenspit.io.git"
  },
  "author": "Jess Sullivan <JessS@lavabit.com",
  "contributors": [
    "KittenSpit.io <xoxd@lavabit.com>",
    "Creative Tim <hello@creative-tim.com>"
  ],
  "license": "AGPL-3.0-or-later",
  "homepage": "https://kittenspit.io/",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "ts-node": "^10.7.0",
    "typescript-require": "^0.3.0",
    "typescript": "^4.6.4"
  }
}
